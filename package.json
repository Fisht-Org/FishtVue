{
  "name": "fishtvue",
  "version": "0.0.1",
  "author": "Egor Bondarenco",
  "description": "Beautiful components that you can use in your applications. Available. Customizable. Open source.",
  "private": false,
  "type": "module",
  "homepage": "https://github.com/Fisht-Org/",
  "repository": {
    "type": "git",
    "url": "https://github.com/Fisht-Org/FishtVue.git"
  },
  "license": "MIT",
  "scripts": {
    "dev": "vite --open src/index.html",
    "preview": "vite preview",
    "build:lib": "NODE_ENV=production rollup -c",
    "pack": "cd dist && npm pack --pack-destination='../'",
    "format": "prettier --write \"**/*.{js,ts,vue,d.ts}\" --cache",
    "format:check": "prettier --check \"**/*.{js,ts,vue,d.ts}\"",
    "lint": "eslint --fix .",
    "lint:fix": "eslint --fix --ext \".js,.vue\" --ignore-path .gitignore .",
    "patch": "npm version patch",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:unit": "vitest --ui",
    "coverage": "vitest run --coverage",
    "prepare": "husky install"
  },
  "keywords": [
    "fishtvue",
    "vue",
    "vue.js",
    "vue3",
    "ui library",
    "component library"
  ],
  "bugs": {
    "url": "https://github.com/Fisht-Org/FishtVue/issues"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.1",
    "@babel/preset-env": "^7.25.2",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^20.14.13",
    "@vitejs/plugin-vue": "^5.1.1",
    "@vitest/coverage-v8": "^2.0.4",
    "@vitest/ui": "^2.0.4",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^6.6.0",
    "eslint-plugin-vue": "^9.27.0",
    "fs-extra": "^11.2.0",
    "husky": "^9.1.4",
    "jsdom": "^24.1.1",
    "path": "^0.12.7",
    "postcss": "^8.4.40",
    "postcss-preset-env": "^9.6.0",
    "postcss-selector-parser": "^6.1.1",
    "prettier": "^3.3.3",
    "rollup": "^4.19.1",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "sass": "^1.77.8",
    "typescript": "^5.4.4",
    "typescript-eslint": "^7.17.0",
    "vite": "^5.3.5",
    "vitest": "^2.0.4",
    "vue-tsc": "^2.0.29"
  },
  "optionalDependencies": {
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.4.0"
  },
  "dependencies": {
    "@heroicons/vue": "^2.1.5",
    "tslib": "^2.6.3",
    "vue": "^3.4.34"
  }
}
